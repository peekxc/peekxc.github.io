= doctype html
html lang="en-us"
  head
    {{ partial "header" . }}
  body data-target="#page_toc"
    {{ partial "navbar" . }}
    .container-fluid.px-0
      .row.m-3
        #main.d-flex.flex-column.col-md-8.border-0.px-3.pt-0  
          = yield main 
          br
        #sidebar.col-md-4.p-0
          = yield sidebar
    {{ partial "js_includes" . }}
    {{ partial "js_search" . }}
  footer.fixed-bottom
    {{ partial "footer" . }}