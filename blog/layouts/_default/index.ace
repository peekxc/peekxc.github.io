= content main
  {{ with .Site.GetPage "/posts" }}
    h3 style="font-style: italic;" {{ .Title }}
    #article_feed
      {{ range (where .RegularPages "File.Ext" "md").ByLastmod }}
        {{ partial "summary" . }}
      {{ end }}
  {{ end }}


= content sidebar 
  #categories.d-flex.flex-column
    {{ partial "list_categories" . }}
  br 
  #tags.d-flex.flex-column
    {{ partial "tags" . }}